{
 "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
 "contentVersion": "1.0.0.0",
 "parameters": {
   "vnetName": {
     "type": "string",
     "defaultValue": "vnet_services_eastus",
     "metadata": {
       "description": "VNet name"
     }
   },
   "vnetAddressPrefix": {
     "type": "string",
     "defaultValue": "10.0.0.0/24",
     "metadata": {
       "description": "Address prefix"
     }
   },
   "GatewayName": {
     "type": "string",
     "defaultValue": "Services-East-GW",
     "metadata": {
       "description": "Services Gateway Name"
     }
   },
   "GatewayPublicIP": {
     "defaultValue": "GW-IP",
     "type": "string",
     "metadata": {
       "description": "Public IP for Azure Gateway"
     }
   },
   "NSG1": {
     "defaultValue": "NSG_services_eastus",
     "type": "string",
     "metadata": {
       "description": "Name of NSG for Web_Users subnet"
     }
   },
   "subnet1Prefix": {
     "type": "string",
     "defaultValue": "10.0.0.0/25",
     "metadata": {
       "description": "Subnet 1 Prefix"
     }
   },
   "subnet1Name": {
     "type": "string",
     "defaultValue": "services_eastus",
     "metadata": {
       "description": "Subnet 1 Name"
     }
   },
   "GatewaysubnetPrefix": {
     "type": "string",
     "defaultValue": "10.0.0.128/27",
     "metadata": {
       "description": "Gateway Subnet Prefix"
     }
   },
   "GatewaysubnetName": {
     "type": "string",
     "defaultValue": "GatewaySubnet",
     "metadata": {
       "description": "Gateway Subnet Name"
     }
   },
   "BastionsubnetPrefix": {
     "type": "string",
     "defaultValue": "10.0.0.160/27",
     "metadata": {
       "description": "Bastion Subnet Prefix"
     }
   },
   "BastionsubnetName": {
     "type": "string",
     "defaultValue": "AzureBastionSubnet",
     "metadata": {
       "description": "Bastion Subnet  Name"
     }
   },
   "location": {
     "type": "string",
     "defaultValue": "[resourceGroup().location]",
     "metadata": {
       "description": "Location for all resources."
     }
   }
 },
 "variables": {},
 "resources": [
   {
     "apiVersion": "2018-10-01",
     "type": "Microsoft.Network/publicIPAddresses",
     "location": "[parameters('location')]",
     "name": "[parameters('GatewayPublicIP')]",
     "properties": {
       "publicIPAllocationMethod": "Dynamic"
     }
   },
   {
     "apiVersion": "2018-10-01",
     "type": "Microsoft.Network/virtualNetworkGateways",
     "location": "[parameters('location')]",
     "name": "[parameters('GatewayName')]",
     "properties": {
       "ipConfigurations": [
         {
           "properties": {
             "subnet": {
               "id": "[resourceId('Microsoft.Network/virtualNetworks/subnets', parameters('VNetName'), parameters('GatewaysubnetName'))]"
             },
             "publicIPAddress": {
               "id": "[resourceId('Microsoft.Network/publicIpAddresses', parameters('GatewayPublicIP'))]"
             }
           },
           "name": "GWipConfig1"
         }
       ],
       "gatewayType": "VPN",
       "vpnType": "RouteBased",
       "enableBgp": false,
       "sku": {
         "name": "VpnGw1",
         "tier": "VpnGw1"
       }
     },
     "dependsOn": [
       "[resourceId('Microsoft.Network/virtualNetworks', parameters('VNetName'))]",
       "[resourceId('Microsoft.Network/publicIpAddresses', parameters('GatewayPublicIP'))]"
     ]
   },
   {
     "name": "[parameters('NSG1')]",
     "type": "Microsoft.Network/networkSecurityGroups",
     "apiVersion": "2018-10-01",
     "location": "[parameters('location')]",
     "properties": {
       "securityRules": [

       ]
     }
   },
   {
     "apiVersion": "2015-05-01-preview",
     "type": "Microsoft.Network/virtualNetworks",
     "name": "[parameters('vnetName')]",
     "location": "[parameters('location')]",
     "properties": {
       "addressSpace": {
         "addressPrefixes": [
           "[parameters('vnetAddressPrefix')]"
         ]
       },
       "subnets": [
         {
           "name": "[parameters('subnet1Name')]",
           "properties": {
             "addressPrefix": "[parameters('subnet1Prefix')]",
             "networkSecurityGroup": {
               "id": "[resourceId('Microsoft.Network/networkSecurityGroups',parameters('NSG1'))]"
             }
           },
           "dependsOn": "[resourceId('Microsoft.Network/networkSecurityGroups',parameters('NSG1'))]"
         },
         {
           "name": "[parameters('GatewaysubnetName')]",
           "properties": {
             "addressPrefix": "[parameters('GatewaysubnetPrefix')]"
           }
         },
         {
           "name": "[parameters('BastionsubnetName')]",
           "properties": {
             "addressPrefix": "[parameters('BastionsubnetPrefix')]"
           }
         }
       ]
     }
   }
 ]
}